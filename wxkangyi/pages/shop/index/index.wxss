.goods_list .goods_box {
  width: calc(50% - 50rpx);
  /* width:100%; */
  background: #fff;
  float: left;
  font-size: 24rpx;
  padding-left: 50rpx;
}
.goods_list .goods_pic {
  width: 290rpx;
  height: 290rpx;
  background: #eee;
  border-radius: 10rpx 10rpx 0 0;
  overflow: hidden;
}
.goods_list .goods_pic ._img {
  width: 100%;
  height: 100%;
}
.goods_info_box {
  width: 290rpx;
}
.goods_list .goods_title {
  line-height: 30rpx;
  height: 60rpx;
  overflow: hidden;
  -webkit-line-clamp: 2;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
}
.goods_list .goods_price {
  width: 40%;
  display: inline-block;
}
.goods_list .goods_sales {
  float: right;
  width: 60%;
  text-align: right;
}
page {
