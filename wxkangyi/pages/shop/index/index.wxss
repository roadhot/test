.title{
  background:#fff;
  width:60%;
  height:60rpx;
  margin:0rpx auto;
  border-radius: 40rpx;
  margin-top:20rpx;
  margin-bottom: 20rpx;
  text-align: center;
  font-size:30rpx;
  font-weight: bold;
  line-height: 60rpx;
}
.line {
  height: 6rpx;
  background-color: #495cbe;
  width: calc(100% - 20%);
  padding: 0 10%;
}
.line .line-col {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
  background-color: #fff;
}
.cate {
  width: calc(100% - 60rpx);
  padding: 20rpx 30rpx;
  flex-wrap: wrap;
  background-color: #495cbe;
  overflow: hidden;
}
.cate .item {
  width:100%;
  align-items: center;
  justify-content: center;
  flex-direction: column;

  min-width: 170rpx;
  float: left;
}
.cate .item image {
  width: 60rpx;
  height: 60rpx;
  display: inline-block;
  margin: auto;
  vertical-align: bottom;
  border-radius: 10rpx;
}
.cate .item .cate_name {
  display: inline-block;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  overflow: hidden;
  white-space: nowrap;
  padding: 0 10rpx;
  font-size: 30rpx;
  color:#fff;
  font-weight: bold;
  
}
.slide {
  /* position: relative; */
  width: 100%;
  height: 400rpx;
  /* padding-top: 20rpx; */
  background-color: #495cbe;
}
.slide .bannerSwiper {
  width:100%;
  height: 400rpx;
}
.slide .imageBanner {
  width: 100%;
