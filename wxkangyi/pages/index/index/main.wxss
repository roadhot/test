.btnlist {
  position: relative;
}
.btnlist .btn {
  width: 100%;
  overflow: hidden;
  height: 400rpx;
}
.btnlist .btn ._img {
  display: block;
  width: 300rpx;
  height: 280rpx;
  margin: auto;
}
.smks {
  margin-top: 50rpx;
  width: 100%;
  /* overflow: hidden; */
  height: 140rpx;
  display: flex;
  justify-content: center;
}
.smks ._button {
  width: 300rpx;
  height: 300rpx;
  line-height: 80rpx;
  border-radius: 100%;
  background-color:rgba(255, 255, 255, 0.1);
  color: #fff;
  font-weight: bold;
  line-height: 300rpx;
  font-size: 60rpx;
}
